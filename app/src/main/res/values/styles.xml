<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="result_pop_scroll">
        <item name="android:background">@color/colorWhite</item>
    </style>
    <style name="result_pop_scrollinner">
        <item name="android:padding">@dimen/result_pop_padding</item>
        <item name="android:rowHeight">12dp</item>
    </style>

    <style name="ocr_theme" parent="Theme.AppCompat.Light.DarkActionBar">
    </style>
    <style name="imageBrowser" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    //悬浮按钮
    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>
